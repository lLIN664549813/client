/* 页面基础样式 */
.publish-sell-page.data-v-b0cdfdc8 {
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100vh;
  background-color: #ffffff;
  overflow: hidden;
  /* 隐藏页面整体滚动条 */
  box-sizing: border-box;
}
.content.data-v-b0cdfdc8 {
  flex: 1;
  box-sizing: border-box;
  overflow: hidden;
}
.content .form-info.data-v-b0cdfdc8 {
  margin-top: 38rpx;
  padding: 0 80rpx;
}
.content .form-info .zhanghu-info.data-v-b0cdfdc8 {
  padding: 38rpx 0;
}
.content .border-top.data-v-b0cdfdc8 {
  border-top: 2rpx solid #D9DBE9;
}
.image-content.data-v-b0cdfdc8 {
  width: 100%;
  position: relative;
  height: 224rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: contain;
  background-position: center center;
  margin-top: 56rpx;
  flex-shrink: 0;
}
/* 覆盖文字：使用百分比 + transform 保证在不同屏幕上位置保持相对稳定 */
.image-content .overlay.data-v-b0cdfdc8 {
  position: absolute;
  top: 18%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  color: #ffffff;
  font-size: 36rpx;
  line-height: 1;
  -webkit-text-size-adjust: none;
}
.image-content .left.data-v-b0cdfdc8 {
  left: 31%;
}
.image-content .right.data-v-b0cdfdc8 {
  right: 11%;
}
@media (max-width: 360px) {
.image-content .overlay.data-v-b0cdfdc8 {
    font-size: 30rpx;
}
}
